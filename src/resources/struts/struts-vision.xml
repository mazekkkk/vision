<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="front" extends="struts-default" namespace="/">
        <action name="blog" class="com.al.vision.action.BlogAction" method="execute">
            <result name="success">blog.jsp</result>
        </action>
        <action name="index" class="com.al.vision.action.IndexAction" method="execute">
            <result name="success">index.jsp</result>
        </action>
        <action name="contact" class="com.al.vision.action.ContactAction" method="execute">
            <result name="success">contact.jsp</result>
        </action>
        <action name="process" class="com.al.vision.action.ProcessAction" method="execute">
            <result name="success">process.jsp</result>
        </action>
        <action name="work" class="com.al.vision.action.WorkAction" method="execute">
            <result name="success">work.jsp</result>
        </action>

    </package>
</struts>